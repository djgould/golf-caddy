{
  "name": "golf-caddy",
  "version": "0.0.0",
  "private": true,
  "description": "Golf Caddy Monorepo",
  "scripts": {
    "build": "turbo build --filter=!@repo/mobile",
    "dev": "turbo dev --filter=!@repo/mobile --concurrency=12",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.0",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=18"
  },
  "prettier": "@repo/prettier-config"
}
