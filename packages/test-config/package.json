{
  "name": "@repo/test-config",
  "version": "0.0.0",
  "private": true,
  "description": "Shared testing configurations and utilities for Golf Caddy",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vitest": {
      "import": "./dist/vitest.mjs",
      "require": "./dist/vitest.js",
      "types": "./dist/vitest.d.ts"
    },
    "./react-native": {
      "import": "./dist/react-native.mjs",
      "require": "./dist/react-native.js",
      "types": "./dist/react-native.d.ts"
    },
    "./setup": {
      "import": "./dist/setup.mjs",
      "require": "./dist/setup.js",
      "types": "./dist/setup.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint .",
    "clean": "rm -rf dist node_modules .turbo"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.0",
    "vitest": "^2.0.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:^",
    "@types/node": "^20.10.0",
    "typescript": "~5.8.3",
    "vite": "^5.0.0"
  }
} 