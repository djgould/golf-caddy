{
  "name": "@repo/db",
  "version": "0.0.0",
  "private": true,
  "description": "Database client and schema for Golf Caddy using Prisma",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./spatial-helpers": {
      "import": "./dist/spatial-helpers.mjs",
      "require": "./dist/spatial-helpers.js",
      "types": "./dist/spatial-helpers.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "postbuild": "prisma generate || true",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "clean": "rm -rf dist node_modules .turbo"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "prisma": "^5.22.0",
    "tsx": "^4.19.4",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0",
    "zod-prisma-types": "^3.2.4"
  }
}
